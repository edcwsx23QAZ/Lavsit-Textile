{
  "name": "lavsit-textile",
  "version": "0.1.0",
  "private": true,
  "description": "Lavsit Textile - парсер данных наличия и цен тканей от разных поставщиков",
  "scripts": {
    "dev": "next dev -p 4001",
    "build": "prisma generate && next build",
    "start": "next start -p 4001",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:init": "tsx scripts/init-suppliers.ts",
    "test:noframes": "tsx scripts/test-noframes.ts",
    "test:noframes-data": "tsx scripts/test-noframes-data.ts",
    "test:noframes-full": "tsx scripts/test-noframes-full.ts",
    "fix:noframes-rules": "tsx scripts/fix-noframes-rules.ts",
    "auto:watch": "tsx scripts/auto-commit-and-push.ts",
    "auto:start": "powershell -ExecutionPolicy Bypass -File scripts/start-auto-commit-service.ps1",
    "auto:stop": "powershell -ExecutionPolicy Bypass -File scripts/start-auto-commit-service.ps1 -Stop",
    "auto:status": "powershell -ExecutionPolicy Bypass -File scripts/start-auto-commit-service.ps1 -Status",
    "auto:commit": "git add -A && git commit -m \"Auto-commit: $(Get-Date -Format 'yyyy-MM-dd HH:mm:ss')\" && git push",
    "revisions:save": "tsx scripts/manage-revisions.ts save",
    "revisions:list": "tsx scripts/manage-revisions.ts list",
    "revisions:cleanup": "tsx scripts/manage-revisions.ts cleanup",
    "revisions:restore": "tsx scripts/manage-revisions.ts restore",
    "setup:auto-commit": "powershell -ExecutionPolicy Bypass -File scripts/setup-auto-commit.ps1",
    "secrets:setup": "powershell -ExecutionPolicy Bypass -File scripts/auto-setup-secrets.ps1",
    "secrets:setup-dry": "powershell -ExecutionPolicy Bypass -File scripts/auto-setup-secrets.ps1 -DryRun",
    "secrets:github-only": "powershell -ExecutionPolicy Bypass -File scripts/auto-setup-secrets.ps1 -GitHubOnly",
    "secrets:vercel-only": "powershell -ExecutionPolicy Bypass -File scripts/auto-setup-secrets.ps1 -VercelOnly"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@tanstack/react-virtual": "^3.13.13",
    "@types/adm-zip": "^0.5.7",
    "adm-zip": "^0.5.16",
    "axios": "^1.7.7",
    "cheerio": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "exceljs": "^4.4.0",
    "glob": "^13.0.0",
    "googleapis": "^144.0.0",
    "imap": "^0.8.19",
    "lucide-react": "^0.454.0",
    "mailparser": "^3.6.5",
    "next": "^14.2.30",
    "puppeteer": "^24.34.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.0",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8",
    "chokidar": "^3.6.0"
  },
  "devDependencies": {
    "@types/imap": "^0.8.40",
    "@types/mailparser": "^3.4.4",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.30",
    "postcss": "^8.4.47",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.7.0",
    "typescript": "^5.5.4"
  }
}
